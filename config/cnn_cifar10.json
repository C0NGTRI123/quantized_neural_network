{
  "model": "cnn",
  "dataset": "cifar10",
  "epochs": 100,
  "batch_size": 128,
  "lr": 0.001,
  "weight_decay": 1e-4,
  "momentum": 0.9,
  "optimizer": "adam",
  "dropout": 0.5,
  "use_batch_norm": true,
  "quantize": true,
  "quantize_only": true,
  "quantize_bits": 4,
  "calibration_samples": 1024,
  "device": "auto",
  "num_workers": 4,
  "seed": 42,
  "log_dir": "logs",
  "log_level": "INFO",
  "log_interval": 100,
  "save_every": 10,
  "data_dir": "./data"
}
